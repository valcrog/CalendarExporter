{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/generate",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
